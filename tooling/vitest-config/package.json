{
  "private": true,
  "version": "1.0.1",
  "name": "@portfolio/vitest-config",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./src/env.ts"
    },
    "./mocks": {
      "types": "./dist/mocks.d.ts",
      "default": "./src/mocks.ts"
    },
    "./msw": {
      "types": "./dist/msw.d.ts",
      "default": "./src/msw.ts"
    },
    "./render": {
      "types": "./dist/render.d.ts",
      "default": "./src/render.tsx"
    },
    "./setup": {
      "types": "./dist/setup.d.ts",
      "default": "./src/setup.ts"
    }
  },
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc --outDir dist",
    "clean": "git clean -xdf .cache .turbo node_modules dist",
    "dev": "tsc --watch --outDir dist",
    "fmt": "biome format --write .",
    "fmt:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@portfolio/testing-mocks": "workspace:*",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@vitejs/plugin-react": "5.1.2",
    "dotenv": "17.2.3",
    "jsdom": "27.4.0",
    "react-router": "7.11.0",
    "regenerator-runtime": "0.14.1",
    "vite": "6.1.0",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "3.2.4"
  },
  "devDependencies": {
    "@portfolio/biome-config": "workspace:*",
    "@portfolio/tsconfig": "workspace:*",
    "@types/node": "25.0.3",
    "typescript": "5.9.3"
  }
}
