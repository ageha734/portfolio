{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "@portfolio/tsconfig/react.json",
    "compilerOptions": {
        "baseUrl": ".",
        "outDir": "./dist",
        "rootDir": ".",
        "paths": {
            "@/*": ["./app/*"],
        },
        "jsx": "react-jsx",
        "allowJs": true,
    },
    "include": ["app/**/*", "docusaurus.config.ts", "sidebars.ts"],
    "exclude": ["node_modules", "dist", "build", ".docusaurus"],
}
