{
    "name": "@portfolio/admin",
    "private": true,
    "type": "module",
    "scripts": {
        "generate": "wrangler types",
        "clean": "git clean -xdf .cache .turbo node_modules dist",
        "dev": "run-p dev:ui dev:page",
        "dev:ui": "storybook dev -p 6007",
        "dev:page": "vite",
        "build": "run-s build:ui build:page",
        "build:ui": "storybook build",
        "build:page": "tsc && vite build",
        "lint": "biome lint .",
        "lint:fix": "biome lint --write .",
        "fmt": "biome format --write .",
        "fmt:check": "biome format .",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "coverage": "vitest run --coverage",
        "e2e": "playwright test",
        "deploy": "wrangler pages deploy ./build --project-name portfolio-admin --branch master"
    },
    "dependencies": {
        "@portfolio/api": "workspace:*",
        "@portfolio/cms": "workspace:*",
        "@portfolio/ui": "workspace:*",
        "@tanstack/react-router": "1.147.3",
        "@trpc/client": "11.0.0",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "4.20260101.0",
        "@portfolio/biome-config": "workspace:*",
        "@portfolio/playwright-config": "workspace:*",
        "@portfolio/storybook-config": "workspace:*",
        "@portfolio/tsconfig": "workspace:*",
        "@portfolio/ui": "workspace:*",
        "@portfolio/vite-config": "workspace:*",
        "@portfolio/vitest-config": "workspace:*",
        "@tanstack/router-plugin": "1.147.3",
        "@tanstack/router-vite-plugin": "1.147.3",
        "@types/react": "18.3.12",
        "@types/react-dom": "18.3.1",
        "@vitejs/plugin-react": "5.1.2",
        "npm-run-all2": "8.0.4",
        "react-router": "7.11.0",
        "storybook": "8.6.14",
        "typescript": "5.9.3",
        "vite": "5.4.0",
        "vitest": "3.2.4",
        "wrangler": "4.54.0"
    },
    "peerDependencies": {
        "wrangler": "4.54.0"
    }
}
