{
  "private": true,
  "name": "portfolio",
  "description": "Portfolio site for ageha734",
  "engines": {
    "bun": "1.1.43",
    "node": "20.19.0"
  },
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "setup": "bun run prepare",
    "prepare": "./scripts/install.sh",
    "generate": "orval",
    "typegen": "wrangler types",
    "dev": "bun run generate && bun run typegen && concurrently -n remix,ui,docs,tsp -c \"blue,green,yellow,purple\" \"bun run dev:remix\" \"bun run dev:ui\" \"bun run dev:docs\" \"bun run dev:tsp\"",
    "dev:remix": "remix vite:dev -p 13000",
    "dev:ui": "storybook dev -p 16006",
    "dev:docs": "docusaurus start -p 30001",
    "dev:tsp": "tsp compile --watch .",
    "build": "bun run generate && bun run typegen && concurrently -n remix,ui,docs -c \"blue,green,yellow\" \"bun run build:remix\" \"bun run build:ui\" \"bun run build:docs\"",
    "build:remix": "remix vite:build",
    "build:ui": "storybook build -o build/ui",
    "build:docs": "docusaurus build",
    "deploy": "bun run generate && bun run typegen && concurrently -n remix,ui,docs -c \"blue,green,yellow\" \"bun run deploy:remix\" \"bun run deploy:ui\" \"bun run deploy:docs\"",
    "deploy:remix": "wrangler pages deploy ./build/remix --project-name portfolio",
    "deploy:ui": "wrangler pages deploy ./build/ui --project-name portfolio-ui --branch master",
    "deploy:docs": "docusaurus deploy",
    "fmt": "concurrently -n fmt:md:fix,fmt:ts:fix,fmt:shell:fix,fmt:actions:fix -c \"blue,green,yellow,purple\" \"bun run fmt:md:fix\" \"bun run fmt:ts:fix\" \"bun run fmt:shell:fix\" \"bun run fmt:actions:fix\"",
    "fmt:check": "concurrently -n fmt:md:check,fmt:ts:check,fmt:shell:check,fmt:actions:check -c \"blue,green,yellow,purple\" \"bun run fmt:md:check\" \"bun run fmt:ts:check\" \"bun run fmt:shell:check\" \"bun run fmt:actions:check\"",
    "fmt:md:check": "remark docs/prompt/ --frail --quiet",
    "fmt:md:fix": "remark docs/prompt/ --output",
    "fmt:ts:check": "biome format ${@:-app/}",
    "fmt:ts:fix": "biome format --write ${@:-app/}",
    "fmt:tsp:check": "tsp format --check ${@:-api/}",
    "fmt:tsp:fix": "tsp format  ${@:-api/}",
    "fmt:shell:check": "go run mvdan.cc/sh/v3/cmd/shfmt@v3.12.0 -l -d ${@:-scripts/*.sh}",
    "fmt:shell:fix": "go run mvdan.cc/sh/v3/cmd/shfmt@v3.12.0 -w ${@:-scripts/*.sh}",
    "fmt:actions:check": "go run github.com/google/yamlfmt/cmd/yamlfmt@v0.20.0 -lint -gitignore_excludes ${@:-.github/**/*.yml}",
    "fmt:actions:fix": "go run github.com/google/yamlfmt/cmd/yamlfmt@v0.20.0 -gitignore_excludes ${@:-.github/**/*.yml}",
    "lint": "concurrently -n lint:md:check,lint:ts:check,lint:shell:check,lint:actions:check -c \"blue,green,yellow,purple\" \"bun run lint:md:check\" \"bun run lint:ts:check\" \"bun run lint:shell:check\" \"bun run lint:actions:check\"",
    "lint:fix": "concurrently -n lint:md:fix,lint:ts:fix,lint:shell:fix,lint:actions:fix -c \"blue,green,yellow,purple\" \"bun run lint:md:fix\" \"bun run lint:ts:fix\" \"bun run lint:shell:fix\" \"bun run lint:actions:fix\"",
    "lint:md:check": "markdownlint-cli2 ${@:-\"docs/prompt/*.md\"}",
    "lint:md:fix": "markdownlint-cli2 --fix ${@:-\"docs/prompt/*.md\"}",
    "lint:ts:check": "biome lint ${@:-app/}",
    "lint:ts:fix": "biome lint --write ${@:-app/}",
    "lint:tsp:check": "tsp compile . --warn-as-error",
    "lint:tsp:fix": "tsp compile .",
    "lint:shell:check": "docker run --rm -v $(pwd):/work -w /work -v /work/node_modules koalaman/shellcheck:v0.11.0 ${@:-scripts/*.sh}",
    "lint:actions:check": "concurrently -n lint:actions:static:check,lint:actions:policy:check -c \"blue,green\" \"bun run lint:actions:static:check\" \"bun run lint:actions:policy:check\"",
    "lint:actions:static:check": "go run github.com/rhysd/actionlint/cmd/actionlint@v1.7.9",
    "lint:actions:policy:check": "go run github.com/suzuki-shunsuke/ghalint/cmd/ghalint@v1.5.4 run ${@:-.github/**/*.yml}",
    "typecheck": "tsc --noEmit",
    "knip": "knip",
    "test": "cross-env NODE_ENV=test vitest run",
    "coverage": "cross-env NODE_ENV=test vitest run --coverage",
    "e2e": "docker build -t e2e -f tests/e2e/Dockerfile . && docker run --rm -e CI=true -v $(pwd):/work -w /work -v $(pwd)/node_modules:/work/node_modules -v $(pwd)/docs/playwright/report:/work/docs/playwright/report e2e bun run e2e:docker",
    "e2e:docker": "playwright test",
    "snapshot": "docker build -t e2e -f tests/e2e/Dockerfile . && docker run --rm -e CI=true -v $(pwd):/work -w /work -v $(pwd)/node_modules:/work/node_modules -v $(pwd)/docs/playwright/report:/work/docs/playwright/report e2e bun run snapshot:docker",
    "snapshot:docker": "playwright test --project=chromium --update-snapshots",
    "interactions": "docker build -t e2e -f tests/e2e/Dockerfile . && docker run --rm -e CI=true -v $(pwd):/work -w /work -v $(pwd)/node_modules:/work/node_modules -v $(pwd)/docs/interactions/report:/work/docs/playwright/report e2e bun run interactions:docker",
    "interactions:docker": "playwright test --config=playwright.storybook.config.ts tests/e2e/storybook/interactions",
    "accessibility": "docker build -t e2e -f tests/e2e/Dockerfile . && docker run --rm -e CI=true -v $(pwd):/work -w /work -v $(pwd)/node_modules:/work/node_modules -v $(pwd)/docs/accessibility/report:/work/docs/playwright/report e2e bun run accessibility:docker",
    "accessibility:docker": "playwright test --config=playwright.storybook.config.ts tests/e2e/accessibility",
    "visual": "docker build -t e2e -f tests/e2e/Dockerfile . && docker run --rm -e CI=true -v $(pwd):/work -w /work -v $(pwd)/node_modules:/work/node_modules -v $(pwd)/docs/visual/report:/work/docs/playwright/report e2e bun run visual:docker",
    "visual:docker": "playwright test --config=playwright.storybook.config.ts tests/e2e/storybook/visual",
    "lighthouse": "lhci autorun",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
