{
    "private": true,
    "name": "portfolio",
    "description": "Portfolio site for ageha734",
    "engines": {
        "bun": "1.1.43",
        "node": "20.19.0"
    },
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "setup": "bun run prepare",
        "prepare": "./scripts/install.sh",
        "generate": "orval",
        "typegen": "wrangler types",
        "dev": "bun run generate && bun run typegen && concurrently -n remix,ui,docs,tsp -c \"blue,green,yellow,purple\" \"bun run dev:remix\" \"bun run dev:ui\" \"bun run dev:docs\" \"bun run dev:tsp\"",
        "dev:remix": "remix vite:dev -p 13000",
        "dev:ui": "storybook dev -p 16006",
        "dev:docs": "docusaurus start -p 30001",
        "dev:tsp": "tsp compile --watch ./api/graphcms.tsp",
        "build": "bun run generate && bun run typegen && concurrently -n remix,ui -c \"blue,green\" \"bun run build:remix\" \"bun run build:ui\"",
        "build:remix": "remix vite:build",
        "build:ui": "storybook build -o build/ui",
        "build:docs": "docusaurus build",
        "deploy": "bun run generate && bun run typegen && concurrently -n remix,ui,env -c \"blue,green,yellow\" \"bun run deploy:remix\" \"bun run deploy:ui\" \"bun run deploy:env\"",
        "deploy:remix": "wrangler pages deploy ./build/client --project-name portfolio --commit-dirty=true",
        "deploy:ui": "wrangler pages deploy ./build/ui --project-name portfolio-ui --branch master",
        "deploy:env": "./scripts/env.sh",
        "fmt": "concurrently -n markdownlint,typescript,shell,actions -c \"blue,green,yellow,purple\" \"bun run fmt:md:fix\" \"bun run fmt:ts:fix\" \"bun run fmt:shell:fix\" \"bun run fmt:actions:fix\"",
        "fmt:check": "concurrently -n markdownlint,typescript,shell,actions -c \"blue,green,yellow,purple\" \"bun run fmt:md:check\" \"bun run fmt:ts:check\" \"bun run fmt:shell:check\" \"bun run fmt:actions:check\"",
        "fmt:md:check": "bun scripts/check.ts --check-type=fmt --lint-type=md",
        "fmt:md:fix": "bun scripts/check.ts --check-type=fmt --lint-type=md --fix",
        "fmt:ts:check": "bun scripts/check.ts --check-type=fmt",
        "fmt:ts:fix": "bun scripts/check.ts --check-type=fmt --fix",
        "fmt:tsp:check": "bun scripts/check.ts --check-type=fmt --lint-type=tsp",
        "fmt:tsp:fix": "bun scripts/check.ts --check-type=fmt --lint-type=tsp --fix",
        "fmt:shell:check": "bun scripts/check.ts --check-type=fmt --lint-type=shell",
        "fmt:shell:fix": "bun scripts/check.ts --check-type=fmt --lint-type=shell --fix",
        "fmt:actions:check": "bun scripts/check.ts --check-type=fmt --lint-type=actions",
        "fmt:actions:fix": "bun scripts/check.ts --check-type=fmt --lint-type=actions --fix",
        "lint": "concurrently -n markdownlint,typescript,shell,actions,textlint -c \"blue,green,yellow,purple,cyan\" \"bun run lint:md:check\" \"bun run lint:ts:check\" \"bun run lint:shell:check\" \"bun run lint:actions:check\" \"bun run lint:textlint:check\"",
        "lint:fix": "concurrently -n markdownlint,typescript,shell,actions,textlint -c \"blue,green,yellow,purple,cyan\" \"bun run lint:md:fix\" \"bun run lint:ts:fix\" \"bun run lint:shell:fix\" \"bun run lint:actions:fix\" \"bun run lint:textlint:fix\"",
        "lint:md:check": "bun scripts/check.ts --check-type=lint --lint-type=md",
        "lint:md:fix": "bun scripts/check.ts --check-type=lint --lint-type=md --fix",
        "lint:ts:check": "bun scripts/check.ts --check-type=lint",
        "lint:ts:fix": "bun scripts/check.ts --check-type=lint --fix",
        "lint:tsp:check": "bun scripts/check.ts --check-type=lint --lint-type=tsp",
        "lint:tsp:fix": "bun scripts/check.ts --check-type=lint --lint-type=tsp --fix",
        "lint:shell:check": "bun scripts/check.ts --check-type=lint --lint-type=shell",
        "lint:actions:check": "bun scripts/check.ts --check-type=lint --lint-type=actions",
        "lint:textlint:check": "bun scripts/check.ts --check-type=lint --lint-type=textlint",
        "lint:textlint:fix": "bun scripts/check.ts --check-type=lint --lint-type=textlint --fix",
        "typecheck": "tsc --noEmit",
        "knip": "knip",
        "check": "concurrently -n unit,snapshot,interactions,accessibility,visual,lighthouse,sonarcloud -c \"blue,green,yellow,purple,cyan\" \"bun run test\" \"bun run e2e\" \"bun run interactions\" \"bun run accessibility\" \"bun run visual\" \"bun run lighthouse\" \"bun run sonarcloud\"",
        "test": "bun scripts/check.ts --check-type=test --lint-type=ts",
        "coverage": "bun scripts/check.ts --check-type=coverage --lint-type=ts",
        "e2e": "docker build -t e2e -f tests/e2e/Dockerfile . && docker run --rm -e CI=true -v $(pwd):/work -w /work -v $(pwd)/node_modules:/work/node_modules -v $(pwd)/docs/playwright/report:/work/docs/playwright/report e2e bun run e2e:docker",
        "e2e:docker": "playwright test",
        "snapshot": "docker build -t e2e -f tests/e2e/Dockerfile . && docker run --rm -e CI=true -v $(pwd):/work -w /work -v $(pwd)/node_modules:/work/node_modules -v $(pwd)/docs/playwright/report:/work/docs/playwright/report e2e bun run snapshot:docker",
        "snapshot:docker": "playwright test --project=chromium --update-snapshots",
        "interactions": "docker build -t e2e -f tests/e2e/Dockerfile . && docker run --rm -e CI=true -v $(pwd):/work -w /work -v $(pwd)/node_modules:/work/node_modules -v $(pwd)/docs/interactions/report:/work/docs/playwright/report e2e bun run interactions:docker",
        "interactions:docker": "playwright test --config=playwright.storybook.config.ts tests/e2e/storybook/interactions",
        "accessibility": "docker build -t e2e -f tests/e2e/Dockerfile . && docker run --rm -e CI=true -v $(pwd):/work -w /work -v $(pwd)/node_modules:/work/node_modules -v $(pwd)/docs/accessibility/report:/work/docs/playwright/report e2e bun run accessibility:docker",
        "accessibility:docker": "playwright test --config=playwright.storybook.config.ts tests/e2e/accessibility",
        "visual": "docker build -t e2e -f tests/e2e/Dockerfile . && docker run --rm -e CI=true -v $(pwd):/work -w /work -v $(pwd)/node_modules:/work/node_modules -v $(pwd)/docs/visual/report:/work/docs/playwright/report e2e bun run visual:docker",
        "visual:docker": "playwright test --config=playwright.storybook.config.ts tests/e2e/storybook/visual",
        "lighthouse": "lhci autorun",
        "with-env": "dotenv -e ../../.env --"
    },
    "dependencies": {
        "@docusaurus/core": "3.9.2",
        "@docusaurus/preset-classic": "3.9.2",
        "@fireworks-js/react": "2.10.8",
        "@graphcms/rich-text-react-renderer": "0.6.2",
        "@mdx-js/react": "3.1.1",
        "@radix-ui/react-dialog": "1.1.15",
