formatter:
  type: basic
  indent: 2
  line_ending: unix
  line_length: 0
  trim_trailing_whitespace: true
  preserve_line_breaks: false
exclude:
  - .cache/**
  - .coverage/**
  - .docusaurus/**
  - .results/**
  - .turbo/**
  - .wrangler/**
  - build/**
  - dist/**
  - node_modules/**
  - report/**
  - results/**
