generator markdown {
  provider = "@portfolio/prisma-markdown"
  output   = "../../../../docs/database/erd.md"
  title    = "データベース設計書"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
